                <RadzenDataGridColumn TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard" Property="LatestActionDate" Title="Latest Action Date">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard" Property="ProjectType" Title="Project Type">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard" Property="JobDescription" Title="Job Description">
                </RadzenDataGridColumn>
