            </RadzenStack>
        </RadzenColumn>
    </RadzenRow>
    <RadzenTextBox Placeholder="Search ..." style="display: block; width: 100%" @oninput="@Search" aria-label="Search by all string columns" />
    <RadzenRow>
      <RadzenColumn SizeMD=6>
     <RadzenCard style="height: 100%">
        <RadzenDataGrid @ref="grid0" ColumnWidth="200px"   AllowFiltering="true" FilterMode="FilterMode.Advanced" AllowPaging="true" AllowSorting="true" ShowPagingSummary="true" PageSizeOptions=@(new int[]{5, 10, 20, 30})
            Data="@(vwFreeTierDashboards)" TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard">
            <Columns>
                <RadzenDataGridColumn TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard" Property="JobNum" Title="Job Num">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard" Property="Borough" Title="Borough">
                </RadzenDataGridColumn>
                <RadzenDataGridColumn TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard" Property="Street" Title="Street">
