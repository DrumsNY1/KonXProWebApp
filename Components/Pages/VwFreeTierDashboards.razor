    </RadzenCard>

    </RadzenColumn>
        <RadzenColumn SizeMD=6>
        <RadzenCard style="height: 100%;margin-top:0px;">
            <RadzenAlert Shade="Shade.Lighter" Variant="Variant.Flat" Size="AlertSize.Small" AlertStyle="AlertStyle.Danger" Visible="@errorVisible">Cannot save VwFreeTierDashboard</RadzenAlert>
            <RadzenTemplateForm TItem="KonXProWebApp.Models.db_9f8bee_konxdev.VwFreeTierDashboard" Data="@(vwFreeTierDashboard)" Visible="@(vwFreeTierDashboard != null)" Submit="@FormSubmit">
                <RadzenRow style="margin-bottom: 1rem">
                    <RadzenColumn SizeMD="3">
                        <RadzenLabel Text="Job Num" Component="JobNum" style="width: 100%" />
                    </RadzenColumn>
                    <RadzenColumn SizeMD="9">
                        <RadzenNumeric style="display: block; width: 100%" @bind-Value="@(vwFreeTierDashboard.JobNum)" Name="JobNum" />
                    </RadzenColumn>
                </RadzenRow>
                <RadzenRow style="margin-bottom: 1rem">
                    <RadzenColumn SizeMD="3">
                        <RadzenLabel Text="Borough" Component="Borough" style="width: 100%" />
                    </RadzenColumn>
                    <RadzenColumn SizeMD="9">
                        <RadzenTextBox style="display: block; width: 100%" @bind-Value="@(vwFreeTierDashboard.Borough)" Name="Borough" />
                    </RadzenColumn>
                </RadzenRow>
                <RadzenRow style="margin-bottom: 1rem">
                    <RadzenColumn SizeMD="3">
                        <RadzenLabel Text="Street" Component="Street" style="width: 100%" />
                    </RadzenColumn>
                    <RadzenColumn SizeMD="9">
                        <RadzenTextBox style="display: block; width: 100%" @bind-Value="@(vwFreeTierDashboard.Street)" Name="Street" />
                    </RadzenColumn>
                </RadzenRow>
                <RadzenRow style="margin-bottom: 1rem">
                    <RadzenColumn SizeMD="3">
                        <RadzenLabel Text="Latest Action Date" Component="LatestActionDate" style="width: 100%" />
                    </RadzenColumn>
                    <RadzenColumn SizeMD="9">
                        <RadzenDatePicker DateFormat="MM/dd/yyyy" style="display: block; width: 100%" @bind-Value="@(vwFreeTierDashboard.LatestActionDate)" Name="LatestActionDate" />
                    </RadzenColumn>
                </RadzenRow>
                <RadzenRow style="margin-bottom: 1rem">
                    <RadzenColumn SizeMD="3">
                        <RadzenLabel Text="Project Type" Component="ProjectType" style="width: 100%" />
                    </RadzenColumn>
                    <RadzenColumn SizeMD="9">
                        <RadzenTextBox style="display: block; width: 100%" @bind-Value="@(vwFreeTierDashboard.ProjectType)" Name="ProjectType" />
                    </RadzenColumn>
                </RadzenRow>
                <RadzenRow style="margin-bottom: 1rem">
                    <RadzenColumn SizeMD="3">
                        <RadzenLabel Text="Job Description" Component="JobDescription" style="width: 100%" />
                    </RadzenColumn>
                    <RadzenColumn SizeMD="9">
                        <RadzenTextBox style="display: block; width: 100%" @bind-Value="@(vwFreeTierDashboard.JobDescription)" Name="JobDescription" />
                    </RadzenColumn>
                </RadzenRow>
                <RadzenRow style="margin-bottom: 1rem">
                    <RadzenColumn SizeMD="3">
                        <RadzenLabel Text="Neighborhood" Component="Neighborhood" style="width: 100%" />
                    </RadzenColumn>
                    <RadzenColumn SizeMD="9">
                        <RadzenTextBox style="display: block; width: 100%" @bind-Value="@(vwFreeTierDashboard.Neighborhood)" Name="Neighborhood" />
                    </RadzenColumn>
