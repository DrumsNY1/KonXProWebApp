   private DOBJobFiling selectedJob;  

   protected override async Task OnInitializedAsync()  
   {  
       await LoadData();  
       selectedJob = jobData.FirstOrDefault(); // Select the first job as default  
   }  

   private async Task LoadData()  
   {  
       try  
       {  
           jobData = new List<DOBJobFiling>  
           {  
               // Sample data as in the original file  
           };  
       }  
       catch (Exception ex)  
       {  
           Console.WriteLine($"Error loading data: {ex.Message}");  
       }  
   }  
}




